@{
    ViewBag.Title = "Home";
}

@if (User.Identity.IsAuthenticated)
{

    <span>Welcome @Html.Encode(User.Identity.Name) </span>
}
else
{
    @Html.ActionLink("Login", "Login", "EMS")
    <br />

    @Html.ActionLink("Register", "Register", "EMS")
}

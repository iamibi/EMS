@{
    ViewBag.Title = "Home";
}
@using Microsoft.AspNetCore.Http;

@if (ViewBag.username != null && ViewBag.username != string.Empty)
{
    Context.Response.Redirect("/EmployeeView");
}
else
{
    @Html.ActionLink("Login", "Login", "EMS")
    <br />

    @Html.ActionLink("Register", "Register", "EMS")
}
